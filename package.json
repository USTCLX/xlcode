{
  "name": "xlcode",
  "version": "0.0.1",
  "description": "A cli tool for quick build a react project",
  "main": "index.js",
  "bin": "./dist/cli.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "rimraf dist && babel lib -d dist --plugins=@babel/plugin-transform-modules-commonjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "react",
    "template"
  ],
  "author": "lixiang",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.2",
    "concat-stream": "^2.0.0",
    "lodash.template": "^4.5.0",
    "map-stream": "0.0.7",
    "tildify": "^2.0.0",
    "vinyl-fs": "^3.0.3",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "rimraf": "^3.0.0"
  }
}